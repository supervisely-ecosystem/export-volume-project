{
  "name": "Export volumes in Supervisely format",
  "type": "app",
  "categories": [
    "export"
  ],
  "description": "Export project or dataset in Supervisely volumes format",
  "docker_image": "supervisely/base-py-sdk:6.17.1",
  "main_script": "src/main.py",
  "modal_template": "src/modal.html",
  "modal_template_state": {
    "downloadVolumes": true,
    "convertSurfaceToMask": true,
    "downloadAnnotations": true,
    "saveInstanceSegmentationMasks": true,
    "saveSemanticSegmentationMasks": true
  },
  "task_location": "workspace_tasks",
  "isolate": true,
  "headless": true,
  "icon": "https://img.icons8.com/color/100/000000/xray.png",
  "icon_background": "#FFFFFF",
  "instance_version": "6.4.48",
  "context_menu": {
    "target": ["volumes_project", "volumes_dataset"],
    "context_root": "Download as"
  }
}
